@inbook{chemical_space2,
author = {Tarcsay, Ákos and Volford, András and Buttrick, Jonathan and Christopherson, Jan-Constantin and Erdős, Máte and Szabó, Zoltán B.},
publisher = {John Wiley & Sons, Ltd},
isbn = {9783527840748},
title = {Navigating Chemical Space},
booktitle = {Computational Drug Discovery},
chapter = {15},
pages = {337-363},
doi = {https://doi.org/10.1002/9783527840748.ch15},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/9783527840748.ch15},
eprint = {https://onlinelibrary.wiley.com/doi/pdf/10.1002/9783527840748.ch15},
year = {2024},
keywords = {drug discovery, chemical space, substructure search, chemical similarity, fingerprint, big data},
abstract = {Summary Drug discovery workflows extensively utilize the collected, standardized, and stored information as well as the immense possibilities stretched by chemically feasible molecules. One key aspect of data exploration is the identification of patterns among chemically related entities. This chapter provides insights into the nature of chemical space and associated data, along with practical elements of navigation-related workflows. Key challenges of the complexity and size of the chemical space are summarized. Chemical structure representations, fingerprinting techniques for similarity search, and substructure matching algorithms are discussed in the context of examples and usage guidelines. Advanced technologies that provide solutions for quick and efficient navigation in the chemical space are highlighted.}
}

